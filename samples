samples.doc.css

body{line-height: 1.5em;}
h1{width:100%;height:48px;line-height:48px;font-size:1.5em;text-align: center;background-color:lavender;}
h2{width:96%;margin:1.5em auto;cursor: pointer;}
.section{width:96%;margin:1.5em auto;display:none;}
h3{width:96%;margin:1.5em auto;margin:0;padding:0;height:24px;line-height: 24px;cursor: pointer;color:#800000;font-style: italic;font-size:0.75em;}
.section > div{display:none;width:96%;margin:1.5em auto;box-shadow: 1px 1px 8px #808080;padding:0.625em;font-size:0.625em;}
.section > div > p{border-bottom: 1px solid #DCDCDC;}

samples.doc.js
$(document).ready(function(){
	var src={"up":'/cross/image/arrow-up.jpg',"down":'/cross/image/arrow-down.jpg'};
	$('h2').on('click',function(){
		if($(this).hasClass('on'))
		{
			$(this).removeClass('on');
			$(this).next('div').hide();
			$(this).next('div').children('h3').removeClass('on');
			$(this).next('div').children('div').hide();

		}else{	$(this).addClass('on');	$(this).next('div').show();	}
	});

	$('h3').on('click',function(){
		if($(this).hasClass('on'))
		{
			$(this).removeClass('on');
			$(this).next('div').hide();
		}else
		{
			$(this).addClass('on');
			$(this).next('div').show();
		}
	});
});

